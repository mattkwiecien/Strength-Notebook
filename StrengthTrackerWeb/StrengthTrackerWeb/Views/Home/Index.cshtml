@{
    ViewData["Title"] = "Home Page";
}
<div ng-controller="newWorkoutController">
    <div class="new-workout-header">
        <h3>Today's Workout {{Test}}</h3>
        <a href="">Save</a> /
        <a href="">Edit</a> /
        <a href="">Delete</a>
    </div>
    <div class="exercise-container container">

        <div class="exercise-row row">
            <div class="exercise-cell col-sm-2">
                <input placeholder="Exercise: Deadlift" />
            </div>
            <div class="exercise-cell col-sm-2">
                <input placeholder="Weight: 135" />
            </div>
            <div class="exercise-cell col-sm-2">
                <input placeholder="Sets: 3" />
            </div>
            <div class="exercise-cell col-sm-2">
                <input placeholder="Set 1" />
            </div>
            <div class="exercise-cell col-sm-2">
                <input placeholder="Set 2" />
            </div>
            <div class="exercise-cell col-sm-2">
                <input placeholder="Set 3" />
            </div>
        </div>
        <div class="exercise-row row">
            <div class="exercise-cell col-sm-2">
                <input placeholder="Exercise: Deadlift" />
            </div>
            <div class="exercise-cell col-sm-2">
                Weight: <input placeholder="135" />
            </div>
            <div class="exercise-cell col-sm-2">
                Sets: <input placeholder="" />
            </div>
        </div>


        <div class="exercise-row row">
            <div class="exercise-cell col-sm-2">
                Exercise: <input placeholder="" />
            </div>
        </div>
    </div>
</div>